async function analyzeEntry(
  text: string,
  emotions: string[] = ["sadness", "joy", "anxiety"]
): Promise<string> {
  const prompt = `Analyze the following journal entry for emotional patterns (${emotions.join(
    ", "
  )}). Respond in 1-2 sentences.\n\nEntry:\n"""${text}"""`;

  try {
    return await callOpenAI(prompt);
  } catch (error) {
    console.error("Error analyzing entry:", error);
    return "Error analyzing entry. Please try again.";
  }
}